    "version": "0.2.0",
    "configurations": [
        // {
        //     "type": "blazorwasm",
        //     "name": "Launch and Debug Blazor WebAssembly Application",
        //     "request": "launch",
        //     "browser": "edge",
        //     "cwd": "${workspaceFolder}/DockerGen"
